<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="459.0" prefWidth="299.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.view_controller.AddPartController">
   <children>
      <Label layoutX="24.0" layoutY="24.0" text="Add Part" textFill="#1e026b">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
      <RadioButton fx:id="rbtnInHouse" layoutX="99.0" layoutY="23.0" mnemonicParsing="false" onAction="#addPartInHouse" selected="true" text="In-House" />
      <RadioButton fx:id="rbtnOutsourced" layoutX="192.0" layoutY="23.0" mnemonicParsing="false" onAction="#addPartOutsourced" text="Outsourced" />
      <TextField fx:id="txtId" disable="true" editable="false" layoutX="60.0" layoutY="59.0" prefHeight="25.0" prefWidth="179.0" promptText="ID" />
      <TextField fx:id="txtPtName" layoutX="60.0" layoutY="117.0" prefHeight="25.0" prefWidth="179.0" promptText="Part Name" />
      <TextField fx:id="txtInv" layoutX="60.0" layoutY="170.0" prefHeight="25.0" prefWidth="179.0" promptText="Inv" />
      <TextField fx:id="txtPPU" layoutX="61.0" layoutY="224.0" prefHeight="25.0" prefWidth="179.0" promptText="Price/Cost" />
      <TextField fx:id="txtMax" layoutX="60.0" layoutY="283.0" prefHeight="25.0" prefWidth="70.0" promptText="Max" />
      <TextField fx:id="txtMin" layoutX="170.0" layoutY="283.0" prefHeight="25.0" prefWidth="70.0" promptText="Min" />
      <TextField fx:id="txtCompanyName" layoutX="60.0" layoutY="338.0" prefHeight="25.0" prefWidth="179.0" promptText="Machine ID" />
      <Button fx:id="btnCancel" layoutX="170.0" layoutY="389.0" mnemonicParsing="false" onAction="#addPartCancel" prefHeight="38.0" prefWidth="70.0" text="Cancel" />
      <Button fx:id="btnSave" layoutX="60.0" layoutY="389.0" mnemonicParsing="false" onAction="#addPartSave" prefHeight="38.0" prefWidth="70.0" text="Save" />
   </children>
</AnchorPane>
